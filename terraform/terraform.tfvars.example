# Example terraform.tfvars file for Team4 Job App Backend
# Copy this file to terraform.tfvars and customize for local development

# Core Infrastructure
location    = "UK South"
environment = "dev"

# Project Configuration  
project_name = "jobapp"
team_name    = "team4"

# Pipeline Configuration (usually set by CI/CD or deploy.sh script)
ci_cd        = false
git_branch   = "local"
build_number = "local-dev"

# Common tags (customize as needed)
common_tags = {
  Project     = "JobApp-Backend"
  Team        = "Team4"
  Repository  = "team4-job-app-backend"
  CostCenter  = "Development"
  Owner       = "YourName"
}